{
    "name": "2dotstwice/value-objects",
    "description": "Classes, traits, interfaces, ... to work with value objects.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Bert Ramakers",
            "email": "bert@2dotstwice.be"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.3",
        "squizlabs/php_codesniffer": "~2.3",
        "phing/phing": "~2.11",
        "satooshi/php-coveralls": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "TwoDotsTwice\\ValueObject\\": "src/"
        }
    },
    "autoload-dev": {
